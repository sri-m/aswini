<h2> Services Records </h2>
<table border="1" width="300">
<% @services.each do |record| %>
<tr>
<td><%= record.name %> </td><td> <%= record.email %></td><td><%= link_to "Show", record %></td><td><%= link_to "Edit", edit_service_path(record) %></td>
<td><%= link_to 'Destroy', record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
<% end %>
</table>
<br />
<%= link_to "Add Service", new_service_path %>

